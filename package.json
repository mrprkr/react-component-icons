{
  "name": "react-component-icons",
  "version": "0.0.1-alpha",
  "description": "Easy configurable icons for react projects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrprkr/reacticons.git"
  },
  "keywords": [
    "icons",
    "react",
    "svg"
  ],
  "author": "@mrprkr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrprkr/reacticons/issues"
  },
  "homepage": "https://github.com/mrprkr/reacticons#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "react": "^17.0.1"
  }
}
